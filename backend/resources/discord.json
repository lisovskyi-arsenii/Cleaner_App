{
  "id": "discord",
  "name": "Discord",
  "description": "Clean Discord caches and local storage",
  "running": false,
  "detect": {
    "type": "dir",
    "paths": [
      "%AppData%\\discord",
      "%APPDATA%\\discord"
    ],
    "registry": []
  },
  "options": [
    {
      "id": "cache",
      "label": "Cache",
      "description": "Discord Cache folder",
      "actions": [
        {
          "command": "delete",
          "search": "walk.files",
          "path": "%AppData%\\discord\\Cache",
          "os": ["windows"]
        }
      ]
    },
    {
      "id": "code_cache",
      "label": "Code Cache",
      "description": "Discord Code Cache folder",
      "actions": [
        {
          "command": "delete",
          "search": "walk.files",
          "path": "%AppData%\\discord\\Code Cache",
          "os": ["windows"]
        }
      ]
    },
    {
      "id": "gpu_cache",
      "label": "GPUCache",
      "description": "Discord GPU cache",
      "actions": [
        {
          "command": "delete",
          "search": "walk.files",
          "path": "%AppData%\\discord\\GPUCache",
          "os": ["windows"]
        }
      ]
    }
  ]
}
